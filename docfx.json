{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.dll"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "pdf": {
 "content": [
   {
     "files": [
       "api/**.yml",
       "api-vb/**.yml"
     ],
     "exclude": [
       "**/toc.yml",
       "**/toc.md"
     ]
   },
   {
     "files": [
       "articles/**.md",
       "articles/**/toc.yml",
       "toc.yml",
       "*.md",
       "pdf/*"
     ],
     "exclude": [
       "**/bin/**",
       "**/obj/**",
       "_site_pdf/**",
       "**/toc.yml",
       "**/toc.md"
     ]
   },
   {
     "files": "pdf/toc.yml"
   }
 ],
 "resource": [
   {
     "files": [
       "images/**"
     ],
     "exclude": [
       "**/bin/**",
       "**/obj/**",
       "_site_pdf/**"
     ]
   }
 ],
 "overwrite": [
   {
     "files": [
       "apidoc/**.md"
     ],
     "exclude": [
       "**/bin/**",
       "**/obj/**",
       "_site_pdf/**"
     ]
   }
 ],
 "dest": "_site_pdf",
"template": ["pdf.default","templates/darkfx"]
}
}